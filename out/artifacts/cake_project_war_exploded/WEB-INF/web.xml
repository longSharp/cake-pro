<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>cake.web.admin.RegistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/admin/regist</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProvinServlet</servlet-name>
        <servlet-class>cake.web.provin.ProvinServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProvinServlet</servlet-name>
        <url-pattern>/provin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CityServlet</servlet-name>
        <servlet-class>cake.web.provin.city.CityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CityServlet</servlet-name>
        <url-pattern>/provin/city</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IndexBannerServlet</servlet-name>
        <servlet-class>cake.web.goods.IndexBannerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexBannerServlet</servlet-name>
        <url-pattern>/goods/Ibanner</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GoodsListServlet</servlet-name>
        <servlet-class>cake.web.goods.GoodsListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoodsListServlet</servlet-name>
        <url-pattern>/goods/list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GoodsDetailServlet</servlet-name>
        <servlet-class>cake.web.goods.GoodsDetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoodsDetailServlet</servlet-name>
        <url-pattern>/goods/detail</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GoodsTypeServlet</servlet-name>
        <servlet-class>cake.web.goods.GoodsTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoodsTypeServlet</servlet-name>
        <url-pattern>/goods/type</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FuzzyCityServlet</servlet-name>
        <servlet-class>cake.web.provin.city.FuzzyCityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FuzzyCityServlet</servlet-name>
        <url-pattern>/provin/fuzzy</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DynamicServlet</servlet-name>
        <servlet-class>cake.web.dynamic.DynamicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DynamicServlet</servlet-name>
        <url-pattern>/dynamics</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DynamicByOneServlet</servlet-name>
        <servlet-class>cake.web.dynamic.DynamicByOneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DynamicByOneServlet</servlet-name>
        <url-pattern>/dynamic</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QualServlet</servlet-name>
        <servlet-class>cake.web.dynamic.QualServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QualServlet</servlet-name>
        <url-pattern>/qual</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CountServlet</servlet-name>
        <servlet-class>cake.web.goods.CountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CountServlet</servlet-name>
        <url-pattern>/goods/count</url-pattern>
    </servlet-mapping>
</web-app>